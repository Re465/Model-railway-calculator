        <Path x:Name="Lein5" Data="M32,325 L35,327.6365" HorizontalAlignment="Left" Margin="17,318,0,0" Stretch="Fill" VerticalAlignment="Top" Width="830" Height="1" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" StrokeThickness="5" Fill="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Stroke="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
        <!--Bit 5-->
        <Label x:Name="LblBit50" Content="Bit 5 (6)&#xD;&#xA;Locomotive address:" HorizontalAlignment="Left" Height="55" Margin="20,318,0,0" VerticalAlignment="Top" Width="160" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <Label x:Name="LblBit52" Content="    0 = Primary address (CV1) (short)&#xA;  32 = Extended address (CV17 and CV 18) (long)" HorizontalAlignment="Left" Height="55" Margin="310,318,0,0" VerticalAlignment="Top" Width="440" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <CheckBox x:Name="ChkBit5" Content="CheckBox" HorizontalAlignment="Left" Height="15" Margin="750,349,0,0" VerticalAlignment="Top" Width="15" Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
        <TextBox x:Name="TxtBit5" HorizontalAlignment="Left" Height="45" Margin="770,318,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="70" BorderThickness="0" Background="{x:Null}" IsEnabled="False" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="36" TextAlignment="Right" BorderBrush="{x:Null}"/>
        <Path x:Name="Lein6" Data="M32,325 L35,327.6365" HorizontalAlignment="Left" Margin="17,368,0,0" Stretch="Fill" VerticalAlignment="Top" Width="830" Height="1" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" StrokeThickness="5" Fill="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Stroke="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
        <!--Bit 6-->
        <!--Bit 7-->
        <!--Einde Bit-->
        <TextBox x:Name="Txt_CV29" HorizontalAlignment="Left" Height="45" Margin="770,505,0,0" TextWrapping="Wrap" Text="2" VerticalAlignment="Top" Width="70" FontSize="36" BorderThickness="0" Background="{x:Null}" IsEnabled="False" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="DarkBlue" TextAlignment="Right" UndoLimit="102" BorderBrush="{x:Null}"/>
        <Label x:Name="LblNote1" Content="Note :" HorizontalAlignment="Left" Height="45" Margin="20,510.21,0,0" VerticalAlignment="Top" Width="90" FontWeight="Bold" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="26"/>
        <Label x:Name="LblNote2" Content="If a bit is not used, the bit should remain at 0" HorizontalAlignment="Left" Height="29" Margin="106,521,0,0" VerticalAlignment="Top" Width="330" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <Label x:Name="Lbladresse0" Content="Please enter address.&#xA;(1 to 9999)" HorizontalAlignment="Left" Height="55" Margin="20,369.992,0,0" VerticalAlignment="Top" Width="160" FontSize="16"/>
        <TextBox x:Name="Txt_Adres" HorizontalAlignment="Left" Height="45" Margin="180,373,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" FontSize="36" Text="3&#xA;" TextAlignment="Center"/>
        <Label x:Name="Lbladresse1" Content="Base station manages primary address up to:" HorizontalAlignment="Left" Height="35" Margin="20,418,0,0" VerticalAlignment="Top" Width="329" FontSize="16"/>
        <Label x:Name="Lbl99" Content="  99&#xD;&#xA;127" HorizontalAlignment="Left" Height="55" Margin="340,418,0,0" VerticalAlignment="Top" Width="45" FontSize="16"/>
        <RadioButton x:Name="Rdb_99" Content="RadioButton" HorizontalAlignment="Left" Height="15" Margin="380,427,0,0" VerticalAlignment="Top" Width="15" FontSize="16"/>
        <RadioButton x:Name="Rdb_127" Content="RadioButton" HorizontalAlignment="Left" Height="15" Margin="380,449,0,0" VerticalAlignment="Top" Width="15" IsChecked="True" FontSize="16"/>
        <Button x:Name="Btn_Calculate" Content="Calculate&#xA;" HorizontalAlignment="Left" Height="35" Margin="284,380,0,0" VerticalAlignment="Top" Width="110" FontSize="20"/>

        <Label x:Name="Lbl_CV1" Content="The value of CV1 is:" HorizontalAlignment="Left" Height="45" Margin="607,386,0,0" VerticalAlignment="Top" Width="159" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <Label x:Name="Lbl_CV17" Content="The value of CV17 is:" HorizontalAlignment="Left" Height="45" Margin="607,431,0,0" VerticalAlignment="Top" Width="159" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <Label x:Name="Lbl_CV18" Content="The value of CV18 is:" HorizontalAlignment="Left" Height="45" Margin="607,477,0,0" VerticalAlignment="Top" Width="159" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <TextBox x:Name="Txt_CV1" HorizontalAlignment="Left" Height="45" Margin="770,369,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="70" FontSize="36" BorderThickness="0" Background="{x:Null}" IsEnabled="False" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="DarkBlue" TextAlignment="Right" UndoLimit="102" BorderBrush="{x:Null}" Visibility="Hidden" Text="3"/>
        <TextBox x:Name="Txt_CV17" HorizontalAlignment="Left" Height="45" Margin="770,415,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="70" FontSize="36" BorderThickness="0" Background="{x:Null}" IsEnabled="False" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="DarkBlue" TextAlignment="Right" UndoLimit="102" BorderBrush="{x:Null}" Visibility="Hidden"/>
        <TextBox x:Name="Txt_CV18" HorizontalAlignment="Left" Height="45" Margin="770,461,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="70" FontSize="36" BorderThickness="0" Background="{x:Null}" IsEnabled="False" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="DarkBlue" TextAlignment="Right" UndoLimit="102" BorderBrush="{x:Null}" Visibility="Hidden"/>
        <Label x:Name="Lbl_CV29" Content="The value of CV29 is:" HorizontalAlignment="Left" Height="45" Margin="607,521,0,0" VerticalAlignment="Top" Width="159" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontSize="16"/>
        <Path x:Name="Lein6_Copy" Data="M32,325 L35,327.6365" HorizontalAlignment="Left" Margin="20,546,0,0" Stretch="Fill" VerticalAlignment="Top" Width="410" Height="1" OpacityMask="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" StrokeThickness="5" Fill="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" Stroke="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>



    </Grid>

